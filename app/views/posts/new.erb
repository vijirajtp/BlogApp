<h2><strong>Post a New Blog</strong></h2>
<div>
  <%= error_messages_for :blog_post %>
  <% form_for :blog_post, @blog_post, :url => posts_path,:html => { :multipart => true } do |f| -%>

    <%= render :partial => 'form', :locals => { :f => f } %>
    
				<p><%= submit_tag 'Post' %></p>
			<% end %>

     <p><%= link_to 'View Blogs', posts_path %></p>
</div>